#include "helper_library/helper.h"


int main(void)
{
    helper_library_init();
    print("Unesite vrednost velicine jedan byte");
    unsigned char x;
    scan("%d", &x);
    print("Uneta vrednost je %d",x);

    int z = (int)x;
    print("Castovana varijabla je: %d", z);
}
